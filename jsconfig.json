  {
    "compilerOptions": {
      "target": "commonjs",
      "allowSyntheticDefaultImports": false,
      "baseUrl": "./",
      "jsx": "react",
      "checkJs": false,
      "paths": {
        "@components/*": [
          "./src/components/*"
        ],
        "@services/*": [
          "./src/services/*"
        ],
        "@utils/*": [
          "./src/utils/*"
        ],
        "@constants/*": [
          "./src/constants/*"
        ],
        "@hooks/*": [
          "./src/hooks/*"
        ],
        "@store/*": [
          "./src/store/*"
        ],
        "@resources/*": [
          "./resources/*"
        ]
      }
    },
    "include": [
      "./src/**/*"
    ]
  }