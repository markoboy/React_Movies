{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "homepage": "http://markoboy.github.io/React_Movies",
  "scripts": {
    "start": "webpack serve --open",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "start:dev": "cross-env NODE_ENV=development npm start",
    "build": "npm run lint && webpack --config webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:dev": "cross-env NODE_ENV=development npm run build -- -w",
    "prebuild:ssr": "npm run build:prod",
    "build:ssr": "cross-env NODE_ENV=production webpack --config configs/webpack.config.server.js --mode=production",
    "start:ssr": "npm run build:ssr",
    "poststart:ssr": "node ./build-server/index.js",
    "dev:build-server": "cross-env NODE_ENV=development webpack --config configs/webpack.config.server.js --mode=development -w",
    "dev:start": "nodemon ./build-server/index.js",
    "dev": "npm-run-all -p build:dev dev:*",
    "serve:dev": "http-server ./dev",
    "serve:prod": "http-server ./build",
    "lint": "eslint ./src",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "predeploy": "npm run build:prod",
    "deploy": "gh-pages -d build",
    "analyzer": "webpack-bundle-analyzer build/stats.json"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
